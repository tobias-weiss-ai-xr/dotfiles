{
    "C_Cpp.updateChannel": "Insiders",
    "update.showReleaseNotes": false,
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "telemetry.telemetryLevel": "off",
    "window.menuBarVisibility": "toggle",
    "workbench.activityBar.visible": true,
    "editor.suggestSelection": "first",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "update.mode": "manual",
    "editor.accessibilitySupport": "off",
    "keyboard.dispatch": "keyCode",
    "python.dataScience.sendSelectionToInteractiveWindow": false,
    "vim.hlsearch": true,
    "vim.history": 100,
    "vim.leader": "<Space>",
    "vim.normalModeKeyBindingsNonRecursive":[
        {
          "before": ["leader", "c"],
          "commands": [
              "workbench.action.files.save",
              "editor.action.commentLine" 
          ]
        },
        {
          "before": ["leader", "w"],
          "commands": [
              "workbench.action.files.save",
          ]
        },
        {
          "before": ["leader", "l", "l"],
          "commands": [
              "workbench.action.files.save",
              "workbench.action.debug.run",
              "workbench.action.terminal.focus"
          ]
        },
        {
          "before": ["leader", "l", "c"],
          "commands": [
              "workbench.action.debug.stop"
          ]
        },
        {
          "before": ["leader", "l", "d"],
          "commands": [
              "workbench.action.files.save",
              "workbench.action.debug.start",
              "workbench.action.focusActiveEditorGroup",
          ]
        },
        {
          "before": ["leader", "l", "m"],
          "commands": [
              "workbench.action.files.save",
              "makefile.outline.launchDebug"
          ]
        },
        {
          "before": ["leader", "l", "f"],
          "commands": [
              "workbench.action.files.save",
              "code-runner.run",
          ]
        },
        {
          "before": ["leader", "f", "f"],
          "commands": [
              "editor.foldAll",
          ]
        },
        {
          "before": ["leader", "f", "t"],
          "commands": [
              "editor.toggleFold",
          ]
        },
        {
          "before": ["leader", "f", "u"],
          "commands": [
              "editor.unfoldAll",
          ]
        },
        {
            "before": ["<Leader>", "/"],
            "commands": [":noh"]
        }
    ],
    "code-runner.executorMap": {
        "python": "python",
        "fsharp": "dotnet fsi",
        "c": "cd $dir && gcc -Wall $fileName -o $fileNameWithoutExt.out && $dir$fileNameWithoutExt.out",
        "cpp": "cd $dir && g++ -std=c++14 $fileName -o $fileNameWithoutExt.out && $dir$fileNameWithoutExt.out",
    },
    "git.autofetch": true,
    "C_Cpp.workspaceSymbols": "All",
    "debug.onTaskErrors": "abort",
    "terminal.integrated.unicodeVersion": "11",
    "terminal.integrated.fontWeightBold": "normal",
    "output.smartScroll.enabled": false,
    "latex-workshop.latex.tools": [
      {
        "name": "latexmk",
        "command": "latexmk",
        "args": [
          "-synctex=1",
          "-interaction=nonstopmode",
          "-file-line-error",
          "-pdf",
          "-outdir=%OUTDIR%",
          "%DOC%"
        ],
        "env": {}
      },
      {
        "name": "lualatex",
        "command": "lualatex",
        "args": [
          "-synctex=1",
          "-interaction=nonstopmode",
          "-file-line-error",
          "%DOC%"
        ],
        "env": {}
      },
      {
        "name": "pdflatex",
        "command": "pdflatex",
        "args": [
          "-synctex=1",
          "-interaction=nonstopmode",
          "-file-line-error",
          "%DOC%"
        ],
        "env": {}
      },
      {
        "name": "bibtex",
        "command": "bibtex",
        "args": [
          "%DOCFILE%"
        ],
        "env": {}
      }
    ],
    "latex-workshop.latex.recipe.default": "lualatex",
    "latex-workshop.latex.recipes": [

      {
        "name": "lualatex ➞ bibtex ➞ lualatex`×2",
        "tools": [
          "lualatex",
          "bibtex",
          "lualatex",
          "lualatex"
        ]
      },
      {
        "name": "pdflatex ➞ bibtex ➞ pdflatex`×2",
        "tools": [
          "pdflatex",
          "bibtex",
          "pdflatex",
          "pdflatex"
        ]
      },
      {
        "name": "lualatex",
        "tools": [
          "lualatex",
        ]
      },
      {
        "name": "latexmk",
        "tools": [
          "latexmk"
        ]
      },
    ],
    "latex-workshop.view.pdf.trim": 2,
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.view.pdf.external.viewer.command": "zathura",
    "latex-workshop.view.pdf.external.viewer.args": [
        "--synctex-editor-command",
        "code --reuse-window -g \"%{input}:%{line}\"",
        "%PDF%"
    ],
    "latex-workshop.view.pdf.external.synctex.command": "zathura",
    "latex-workshop.view.pdf.external.synctex.args": [
        "--synctex-forward=%LINE%:0:%TEX%",
        "%PDF%"
    ],
    "workbench.colorTheme": "Visual Studio Dark - C++",
    "python.languageServer": "Pylance",
    "window.zoomLevel": 2,
    "terminal.external.linuxExec": "xfce4-terminal",
    "python.terminal.activateEnvInCurrentTerminal": true,
    "terminal.integrated.inheritEnv": false,
    "cmake.configureOnOpen": true,
    "latex-workshop.latex.build.forceRecipeUsage": true,
}